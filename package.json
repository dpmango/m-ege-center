    "svg4everybody": "^2.1.9",
    "swiper": "^5.4.5",
    "uuid": "^8.3.2",
    "v-mask": "^2.3.0",
    "v-scroll-lock": "^1.3.1",
    "vee-validate": "^3.4.13",
    "vue": "^2.6.14",
    "vue-awesome-swiper": "^4.1.1",
    "vue-router": "^3.5.3",
    "vue-select": "^3.16.0",
    "vue-slide-up-down": "^2.0.1",
